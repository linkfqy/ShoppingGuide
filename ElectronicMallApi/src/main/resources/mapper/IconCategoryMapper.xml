<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rabbiter.em.mapper.IconCategoryMapper">
    <select id="getCategoryIdsByIconId" resultType="long">
        SELECT category_id FROM icon_category
        WHERE icon_id = #{iconId}
    </select>
</mapper>
